{
  "name": "@epimodev/callbag-doki",
  "version": "0.1.0",
  "description": "lib to create observable and iterable based on callbag specs",
  "author": "Frederic Godin",
  "license": "MIT",
  "main": "index.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "keywords": [
    "callbag",
    "stream",
    "iterator",
    "typescript"
  ],
  "files": [
    "operators",
    "sources",
    "utils",
    "index.js",
    "index.d.ts"
  ],
  "engines": {
    "node": ">=11.7.0"
  },
  "engineStrict": true,
  "scripts": {
    "build": "tsc --project './tsconfig.build.json' && gulp && rm -rf ./module",
    "test": "jest",
    "lint": "tslint -c tslint.json 'src/**/*.ts?(x)'",
    "check-types": "tsc --noEmit",
    "check-format": "prettier --list-different 'src/**/*.ts?(x)'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn check-format"
    }
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/query-string": "^5.1.0",
    "gulp": "^4.0.2",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "shelljs": "^0.8.3",
    "through2": "^3.0.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "callbag": "^1.2.0",
    "query-string": "^5.1.1"
  }
}
