{
  "name": "callbag-doki",
  "version": "0.1.0",
  "description": "lib to create observable and iterable based on callbag specs",
  "author": "Frederic Godin",
  "license": "MIT",
  "main": "index.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "keywords": [
    "callbag",
    "stream",
    "iterator",
    "typescript"
  ],
  "files": [
    "src",
    "operators",
    "sources",
    "utils",
    "index.js",
    "index.d.ts"
  ],
  "engines": {
    "node": ">=11.7.0"
  },
  "engineStrict": true,
  "scripts": {
    "build": "tsc && gulp && rm -rf ./module",
    "lint": "tslint -c tslint.json 'src/**/*.ts?(x)'",
    "check-types": "tsc --noEmit",
    "check-format": "prettier --list-different 'src/**/*.ts?(x)'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn check-format"
    }
  },
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-terser": "^1.2.0",
    "husky": "^3.0.0",
    "prettier": "^1.18.2",
    "shelljs": "^0.8.3",
    "terser": "^4.1.2",
    "through2": "^3.0.1",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "callbag": "^1.2.0"
  }
}
